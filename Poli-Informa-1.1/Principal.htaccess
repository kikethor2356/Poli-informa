RewriteEngine On
RewriteBase /

# Si la URL solicitada no es para Login/index.php
RewriteCond %{REQUEST_URI} !^/Cliente/Login/index\.php$
# Redirigir a Login/index.php
RewriteRule ^.*$ /Cliente/Login/index.php [L]

# Si la URL solicitada no es para Login/index.php
RewriteCond %{REQUEST_URI} !^/Administrador/Login/index\.php$
# Redirigir a Login/index.php
RewriteRule ^.*$ /Administrador/Login/index.php [L]
